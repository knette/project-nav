<div class="page-header">

    <h1><%= user.name %></h1>

    <a href="/users/logout" class="btn btn-default btn-sm">Logout</a>

</div>
<div class="row">
    <div class="col-sm-6 col-sm-offset-3" style="margin: 50px auto">
        <div class="well">

            <h3>Locally Stored User</h3>

            <p>
                <img class="profile-pic" src= <%= user.picture %>/>
                <strong>Name</strong>: <%= user.name %><br>
                <strong>id</strong>: <%= user._id %><br>
                <strong>email</strong>: <%= user.email %><br>
                <strong>password</strong>: <%= user.password %>
            </p>
                
        </div>
    </div>
</div>