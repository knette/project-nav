Projects: <h1><%= projects.length %><span id="newProject"><a href="/projects/new">New Project</a></span></h1>

<% projects.forEach((p)=>{ %>

    <div class="card" style="width: 20rem;">
            <img class="card-img-top" src= <%= p.imageUrl %> alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title"><%= p.name %></h5>
                <p class="card-text"><%= p.description %></p>
                <a href="/projects/<%= p._id%>" class="btn btn-primary">View Project</a>
            </div>
        </div>
<% }) %>
</ul>